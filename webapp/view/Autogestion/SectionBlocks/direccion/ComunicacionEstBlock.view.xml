				<Text text="{/MaestroProveedores/0/ComMovil}" />
				<Label text="Pais del telefono" />
				<Text text="{/MaestroProveedores/0/ComPaisTelefono}" />
				<Label text="Telefono" />
				<Text text="{/MaestroProveedores/0/ComTelefono}" />
				<Label text="Correo" />
